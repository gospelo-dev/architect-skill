{
  "name": "gospelo-architect",
  "version": "0.1.5",
  "description": "AI-powered diagram generator for system architecture, UML, and infrastructure visualization",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gospelo-architect": "./bin/cli.ts"
  },
  "files": [
    "dist",
    "!dist/data",
    "!dist/src/data",
    "!dist/cli.js",
    "bin",
    "src",
    "!src/icons",
    "!src/data"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build",
    "build:skill": "mkdir -p dist/skills && cd .github/skills/gospelo-architect && zip -r ../../../dist/skills/gospelo-architect-skill.zip . -x '*.DS_Store' && cd ../../.. && echo 'Created: dist/skills/gospelo-architect-skill.zip'"
  },
  "keywords": [
    "diagram",
    "svg",
    "architecture",
    "uml",
    "aws",
    "azure",
    "gcp",
    "visualization",
    "ai",
    "claude",
    "zero-dependency"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gospelo-dev/architect-skill.git"
  },
  "homepage": "https://github.com/gospelo-dev/architect-skill#readme",
  "bugs": {
    "url": "https://github.com/gospelo-dev/architect-skill/issues"
  },
  "author": "gorosun",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "dependencies": {}
}
